  <FlowNode>
    <NodeID>1963</NodeID>
    <NodeType>SCRIPT</NodeType>
    <NodeName>脚本 SIP 注册</NodeName>
    <ExtendedProperty>ZoneType=0;ScriptContent=ini_file = '/home/hesong/ipsc/config/{}/SipReg.ini'.format(GetIpscID())
for i in range(64):
	section = str(i)
	username = IniGetString(section, 'username', None, ini_file)
	if username is None:
		break
	displayname = IniGetString(section, 'displayname', username, ini_file)
	psw = IniGetString(section, 'psw', None, ini_file)
	if psw is None:
		TraceErr('No psw field in sip account %d' % i)
		continue
	authorusername = IniGetString(section, 'authorusername', '', ini_file)
	domain = IniGetString(section, 'domain', None, ini_file)
	if domain is None:
		TraceErr('No domain field in sip account %d' % i)
		continue
	proxyip = IniGetString(section, 'proxyip', domain, ini_file)
	proxyport = IniGetInt(section, 'proxyport', 5060, ini_file)
	expires = IniGetInt(section, 'expires', 3600, ini_file)
	SipRegister(displayname, username, psw, authorusername, proxyip, proxyport, domain, expires)
</ExtendedProperty>
    <PropertySet>
